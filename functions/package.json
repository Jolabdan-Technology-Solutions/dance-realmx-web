{
  "name": "dance-realm-functions",
  "version": "1.0.0",
  "description": "Dance Realm Cloud Functions",
  "main": "lib/index.js",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
