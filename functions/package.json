    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.1.1",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.3",
    "stripe": "^18.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.22",
    "@types/multer": "^1.4.12",
    "@types/nodemailer": "^6.4.17",
    "@types/stripe": "^8.0.416",
    "firebase-functions-test": "^3.3.0",
    "typescript": "^5.7.3"
  },
  "private": true
